{
  "extends": "airbnb-base",
  "globals": {
    "proton": true,
    "passport": true,
    "Sparkd": true,
    "Language": true,
    "Version": true,
    "User": true,
    "Token": true,
    "Like": true,
    "Report": true,
    "Feedback": true,
    "Purchase": true,
    "TestUser": true,
    "describe": true,
    "it": true
  },
  "rules": {
    "func-names": 0,
    "semi": ["error", "never"],
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "new-cap": 0,
    "no-param-reassign": 0
  }
}
